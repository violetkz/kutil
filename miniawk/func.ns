
def s(a , b) {
    if (a > b) {
        return a+b;
    }
    else {
        return b - a;
    }
}

def fn (n) {
    if (n == 0 or n == 1) {
        return n;
    }
    return fn(n - 1)+ fn(n-2);    
}

main {
      print("func test");
      print("fn(3):", fn(3));
      print("fn(10):", fn(10));
